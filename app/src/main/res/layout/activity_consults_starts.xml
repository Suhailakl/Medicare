<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_consult_starts">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/cS_layout">
    <ProgressBar
        android:id="@+id/consult_doc_pro"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="270dp"
        android:layout_height="270dp"
        android:layout_centerInParent="true"
        android:background="@drawable/circle_dark_blue"
        android:indeterminate="false"
        android:max="100"
        android:progress="100"
        android:progressDrawable="@drawable/drawable_circle_yellow"
        android:rotation="-90" />

    <TextView
        android:id="@+id/consult_hour_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:textSize="30dp"
        android:text="Text" />

    <TextView
        android:id="@+id/consult_day_left"
        android:textSize="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp"
        android:layout_alignBaseline="@+id/consult_hour_left"
        android:layout_alignBottom="@+id/consult_hour_left"
        android:layout_toLeftOf="@+id/consult_hour_left"
        android:layout_toStartOf="@+id/consult_hour_left" />
    <TextView
        android:id="@+id/consult_min_left"
        android:textSize="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:layout_alignBaseline="@+id/consult_hour_left"
        android:layout_alignBottom="@+id/consult_hour_left"
        android:layout_toRightOf="@+id/consult_hour_left"
        android:layout_toEndOf="@+id/consult_hour_left"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />

    <TextView
        android:id="@+id/consult_sec_sry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sry"
        android:layout_marginTop="200dp"
        android:visibility="gone"
        android:textSize="30dp"
        android:layout_centerHorizontal="true" />
    <TextView
        android:id="@+id/consult_strtsIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="210dp"
        android:textColor="@color/colorPrimary"
        android:textSize="30dp"
        android:text="@string/strtsIn" />
    <TextView
        android:id="@+id/consult_sec_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:textSize="30dp"
        android:layout_marginTop="31dp"
        android:layout_below="@+id/consult_hour_left"
        android:layout_toRightOf="@+id/consult_day_left"
        android:layout_toEndOf="@+id/consult_day_left" />

</RelativeLayout>
    <TextView
        android:id="@+id/consult_sec_noBk"
        android:textSize="20dp"
        android:visibility="gone"
        android:layout_marginTop="250dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Noptnts"
        android:layout_centerHorizontal="true" />
    <ProgressBar
        android:id="@+id/cS_progress"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" />
    <org.adw.library.widgets.discreteseekbar.DiscreteSeekBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cS_progress"
        android:id="@+id/sC_seek"
        android:layout_marginTop="500dp"
        android:visibility="gone"
        app:dsb_min="1"
        app:dsb_max="7"
        />

</RelativeLayout>